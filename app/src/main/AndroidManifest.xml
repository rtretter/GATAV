<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.hskl.gatav.flappybender">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.SkinActivity"></activity>
        <activity android:name=".views.HighscoreActivity" />
        <activity android:name=".views.GameOverActivity" />
        <activity android:name=".views.OptionsActivity" />
        <activity android:name=".views.MenuActivity" />
        <activity android:name=".views.PreActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.MainActivity" />

        <service android:name=".sound.Discman" />
    </application>

</manifest>